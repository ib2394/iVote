<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddCandidateServlet</servlet-name>
        <servlet-class>controller.AddCandidateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CandidateListServlet</servlet-name>
        <servlet-class>controller.CandidateListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCandidateServlet1</servlet-name>
        <servlet-class>controller.AddCandidateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCandidateServlet</servlet-name>
        <servlet-class>controller.DeleteCandidateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddCandidateServlet</servlet-name>
        <url-pattern>/AddCandidateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CandidateListServlet</servlet-name>
        <url-pattern>/CandidateListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCandidateServlet1</servlet-name>
        <url-pattern>/AddCandidateServlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCandidateServlet</servlet-name>
        <url-pattern>/DeleteCandidateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
